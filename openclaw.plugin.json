{
  "id": "clawdbot-discord",
  "name": "clawdbot-discord",
  "version": "1.0.0",
  "description": "Discord plugin with proxy support for Clawdbot",
  "author": "Yoimiya",
  "license": "MIT",
  "type": "channel",
  "channels": ["clawdbot-discord"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "openclaw": ">=1.0.0"
  },
  "keywords": ["discord", "channel", "proxy"],
  "repository": "",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "token": {
        "type": "string",
        "description": "Discord bot token"
      },
      "proxyUrl": {
        "type": "string",
        "description": "Proxy URL (http, https, socks)"
      },
      "intents": {
        "type": "array",
        "items": { "type": "string" },
        "description": "Gateway intents",
        "default": ["GUILD_MESSAGES", "DIRECT_MESSAGES", "MESSAGE_CONTENT"]
      },
      "autoReconnect": {
        "type": "boolean",
        "description": "Auto reconnect on disconnect",
        "default": true
      },
      "heartbeatInterval": {
        "type": "number",
        "description": "Heartbeat interval in milliseconds",
        "default": 45000
      }
    },
    "required": []
  }
}
