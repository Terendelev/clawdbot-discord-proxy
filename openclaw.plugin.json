{
  "id": "clawdbot-discord-proxy",
  "name": "Discord Proxy",
  "version": "1.0.0",
  "description": "Discord channel with proxy support for Clawdbot",
  "author": "Yoimiya",
  "license": "MIT",
  "type": "channel",
  "channels": ["clawdbot-discord-proxy"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "openclaw": ">=1.0.0"
  },
  "keywords": ["discord", "channel", "proxy"],
  "repository": "",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "token": {
        "type": "string",
        "description": "Discord bot token"
      },
      "proxyUrl": {
        "type": "string",
        "description": "Proxy URL (http, https, socks)"
      },
      "enabled": {
        "type": "boolean",
        "description": "Enable this account",
        "default": true
      },
      "name": {
        "type": "string",
        "description": "Account display name"
      }
    },
    "required": []
  }
}
